{
  "name": "geotoolbox",
  "version": "3.0.3",
  "description": "geotoolbox is GIS javascript library. It is based on d3geo, topojson and geos-wasm.",
  "main": "src/index.js",
  "module": "src/index.js",
  "jsdelivr": "dist/index.min.js",
  "unpkg": "dist/index.min.js",
  "exports": {
    "umd": "./dist/index.min.js",
    "default": "./src/index.js"
  },
  "type": "module",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "rollup --config",
    "prepare": "npm run build",
    "docs": "jsdoc --configure jsdoc.json --verbose",
    "test": "node test/test.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neocarto/geotoolbox.git"
  },
  "keywords": [
    "GIS",
    "geoJSON",
    "geospatial",
    "geocomputing",
    "geography"
  ],
  "author": "Nicolas Lambert",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/neocarto/geotoolbox/issues"
  },
  "homepage": "https://github.com/neocarto/geotoolbox#readme",
  "devDependencies": {
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "json-diff": "^1.0.6",
    "lodash": "^4.17.21",
    "minami": "^1.2.3",
    "rollup-plugin-terser": "^7.0.2"
  },
  "dependencies": {
    "d3-array": "^3.2.4",
    "d3-dsv": "^3.0.1",
    "d3-geo": "^3.0.1",
    "docdash": "^2.0.2",
    "gdal3.js": "^2.8.1",
    "geojson-precision": "^1.0.0",
    "geos-wasm": "^1.1.6",
    "jsdoc": "^4.0.4",
    "topojson-client": "^3.1.0",
    "topojson-server": "^3.0.1",
    "topojson-simplify": "^3.0.3"
  },
  "peerDependencies": {
    "gdal3.js": "^3.0.0"
  }
}
